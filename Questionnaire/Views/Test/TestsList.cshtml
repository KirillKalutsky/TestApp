@using Questionnaire.Models.Dto;
@model List<TestDto>;

@foreach(var test in Model)
{
    <div class="test">
        <div class="header">
            <h2>@test.Title</h2>
        </div>
        <div>
            @Html.ActionLink("Перейти к тесту", "Index", "Test", new { id = @test.Id}, new { @class = "button"})
            @Html.ActionLink("Перейти к результатам теста", "GetTestResults", "Test", new { id = @test.Id}, new { @class = "button"})
        </div>
    </div>
}

